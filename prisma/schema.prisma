

generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"  // <- required in v5/v6

}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id Int @id @default(autoincrement())
  name String
  age Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}



